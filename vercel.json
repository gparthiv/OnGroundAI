{
    "buildCommand": "echo 'Build complete'",
    "outputDirectory": "frontend",
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        },
        {
            "src": "frontend/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "main.py"
        },
        {
            "src": "/run_agent",
            "dest": "main.py"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/frontend/assets/$1"
        },
        {
            "src": "/(.*\\.(css|js|html|png|jpg|svg|ico))",
            "dest": "/frontend/$1"
        },
        {
            "src": "/",
            "dest": "/frontend/index.html"
        }
    ]
}